//forEach --> Higher Order Method (function + object/DS )

let arr = [1,2,3,4,5];

//funcName.forEach(Callback function)


arr.forEach((val, idx) => { //val ==> value at each index
    console.log(val + 10, idx);
    
})
console.log();


//Practice Problem Solve --> Print Square

let ar = [1,2,3,4,5];

ar.forEach((val) => {
    console.log(val*val);
    
})